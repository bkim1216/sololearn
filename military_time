time = input().split()

def am(time):
    normal = time[0].split(':')
    if int(normal[0]) < 10:
        return f"0{normal[0]}:{normal[1]}"
    else:
        return f"{normal[0]}:{normal[1]}"

def pm(time):
    military = time[0].split(':')
    return f"{int(military[0]) + 12}:{military[1]}"


def convert(time):
    if time[1] == 'AM':
        return am(time)
    else:
        return pm(time)

print(convert(time))
